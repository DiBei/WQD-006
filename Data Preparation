bankbehavir<- read.csv("bankbehavir.csv", header = TRUE, sep = ".")
bankbehavir <- subset(bankbehavir, select = -c(ID))
set.seed(42)
split = sample.split(bankbehavir$y, SplitRatio = 0.8)
train_set = subset(bankbehavir, split == TRUE)
test_set = subset(bankbehavirdf, split == FALSE)

bankbehavir$marital <-as.factor(bankbehavir$marital)
bankbehavir$job <-as.factor(bankbehavir$job)
bankbehavir$education <-as.factor(bankbehavir$education)
bankbehavir$default <-as.factor(bankbehavir$default)
bankbehavir$housing <-as.factor(bankbehavir$housing)
bankbehavir$loan <-as.factor(bankbehavir$loan)
bankbehavir$contact <-as.factor(bankbehavir$contact)
bankbehavir$month <-as.factor(bankbehavir$month)
bankbehavir$poutcome<-as.factor(bankbehavir$poutcome)
